@model List<Models.EntityFrameWork.Menu>
<div class="call">
    <p><span>Cần giúp đỡ?</span> Liên hệ chúng tôi <span class="number">0123456789</span></p>
    </div>
<div class="account_desc">
   @{
       var session = (UserLogin)Session[ShopHoa.Common.CommonConstant.USER_SESSION];
}
@if (session == null)
{
    <ul>
        @foreach (var item in Model)
        {
            <li><a href="@item.Link" target="@item.Target">@item.Text</a></li>
        }

    </ul>
}
else
{
    <ul>
        <li>Xin chào:@session.UserName</li>
        <li>@Html.ActionLink("Sửa", "Edit", "User")</li>
        <li>@Html.ActionLink("Thoát", "Logout", "User")</li>
    </ul>
}
   
</div>
<div class="clear"></div>
